<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - AgroTool</title>
    
    @Scripts.Render("~/bundles/jquery")
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/backend.css" type="text/css" media="screen" />
    <link href="~/Content/ErrorPopUp.css" rel="stylesheet" />
    
    @Scripts.Render("~/bundles/modernizr")

    <style>
        body { padding-top:50px; }
    </style>

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements. All other JS at the end of file. -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
    <![endif]-->
</head>
<body ng-app="APP">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand logo" href="/admin">
                    <img src="/Images/logo-light.png" />
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Productos", "Productos", "Admin")</li>
                    <li>@Html.ActionLink("Atributos", "Atributos", "Admin")</li>
                    <li>@Html.ActionLink("Equivalencias de Atributos", "Equivalencias", "Admin")</li>
                    <li>@Html.ActionLink("Cargar Ensayos", "Ensayos", "Admin")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jqueryui")
    <script src="/Scripts/core.js"></script>
    <script src="/Scripts/angular.js"></script>
    <script src="/Scripts/angular-resource.js"></script>
    <script src="/Scripts/ng/AgrotoolBackendApp.js"></script>
    <script src="/Scripts/ng/ProductosCtrl.js"></script>
    <script src="/Scripts/ng/ProductoAtributosCtrl.js"></script>
    <script src="/Scripts/ng/ProductoLugaresCtrl.js"></script>
    <script src="/Scripts/ng/AtributosCtrl.js"></script>
    <script src="/Scripts/ng/EnsayosCtrl.js"></script>
    <script src="~/Scripts/jquery.fancybox.pack.js"></script>
    <script src="~/Scripts/jquery.placeholder.min.js" type="text/javascript"></script>
    <script src="/Scripts/underscore.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>

    <script src="~/Scripts/angucomplete.js"></script>
    <script src="~/Scripts/ui-bootstrap-0.10.0.js"></script>
    <script src="~/Scripts/ui-bootstrap-0.10.0.min.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-0.10.0.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-0.10.0.min.js"></script>
    @RenderSection("scripts", required: false)

     @{Html.RenderPartial("Popups");}

</body>
</html>
